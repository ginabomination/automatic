<!-- index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Year 7 House Points Breakdown</title>
</head>

    <h1>Year 7 House Points Breakdown</h1>
    <body>
      <button onclick="goBack()">Back</button>
      <script>
      function goBack() {
        window.history.back();
      }
      </script>
      <p>First name, Last name, Tutor group, House points</p>
      </body>

</body>
<ul class="names">
  <!-- Loop through quotes -->
  <% for(var i = 0; i < names.length; i++) {%>
    <li class="House">
      <span><%= names[i].Firstname %></span>
      <span><%= names[i].Fullname %></span>:
      <span><%= names[i].Tutorgroup %></span>
      <!-- Output quote from the iterated quote object -->
      <span><%= names[i].Points %></span>
    </li>
  <% } %>
</ul>
</body>

</html>